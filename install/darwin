#!/bin/bash

$BREW cask install \
    1password \
    brave-browser \
    homebrew/cask-versions/brave-browser-nightly \
    cryptomator \
    cyberduck \
    docker \
    protonvpn \
    rambox \
    viscosity \
    visual-studio-code \

echo "Create symlink for gitconfig"
ln -sf $PWD/config/gitconfig-darwin $HOME/.gitconfig

echo "Create symlink for zshrc"
ln -sf $PWD/config/zshrc-darwin $HOME/.zshrc
