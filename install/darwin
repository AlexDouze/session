#!/bin/bash

$BREW cask install \
    1password \
    brave-browser \
    cryptomator \
    cyberduck \
    docker \
    protonvpn \
    rambox \
    viscosity \
    visual-studio-code \

echo "Create symlink for gitconfig"
ln -sf $PWD/config/gitconfig-darwin $HOME/.gitconfig

echo "Create symlink for tmux configuration"
ln -sf $PWD/config/tmux-darwin.conf $HOME/.tmux.conf

